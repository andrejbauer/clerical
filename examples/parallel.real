function parallel():
  case
  | (var i := 0 in while i < 100000 do i := i + 1 end; false) => true
  | (var i := 0 in while i < 100000 do i := i + 1 end; false) => true
  | (var i := 0 in while i < 100000 do i := i + 1 end; false) => true
  | (var i := 0 in while i < 100000 do i := i + 1 end; true) => true
  end

do parallel()
